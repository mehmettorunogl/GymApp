@model Contact
<h1>İletişim Silme</h1>
<p>@Model.Title</p>
<p>@Model.Description</p>
<p>@Model.ImageUrl</p>
<p>@Model.InstagramRedirectUrl</p>
<p>@Model.XRedirectUrl</p>
<p>@Model.LinkedinRedirectUrl</p>
<p>@Model.CreatedDate</p>
<p>@Model.UpdatedDate</p>

<h1>Silmek istediğinize emin misiniz?</h1>

<form method="post">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Sil" class="btn btn-danger" />
</form>

